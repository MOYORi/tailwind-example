{
  "name": "tailwind-example",
  "version": "0.1.2",
  "description": "A tailwind starter package for rapid design prototyping for concrete5",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:c5": "cross-env NODE_ENV=production webpack --config webpack.config.js --env.c5",
    "watch": "cross-env NODE_ENV=development webpack --config webpack.config.js --watch --progress --hide-modules",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hide-modules --watch --hot --config=webpack.config.js"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "css",
    "webpack",
    "purgecss",
    "postcss"
  ],
  "author": "Derek Cameron",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "extract-loader": "^4.0.3",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "tailwindcss": "^1.3.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {}
}
